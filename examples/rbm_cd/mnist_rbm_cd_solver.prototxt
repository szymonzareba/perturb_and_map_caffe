net: "examples/rbm_cd/mnist_rbm_cd.prototxt"
test_iter: 100
test_interval: 500
test_compute_loss: true
base_lr: 0.01
lr_policy: "fixed"
gamma: 1
stepsize: 10000
display: 100
max_iter: 10000
weight_decay: 0
snapshot: 1000
snapshot_prefix: "examples/rbm_cd/mnist_rbm_cd"
momentum: 0.9
# solver mode: CPU or GPU
solver_mode: GPU
