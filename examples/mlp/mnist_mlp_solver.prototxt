net: "examples/mlp/mnist_mlp.prototxt"
test_iter: 100
test_interval: 500
# test_compute_loss: true
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 10000
display: 100
max_iter: 65000
weight_decay: 0.0001
snapshot: 10000
snapshot_prefix: "examples/mlp/mnist_mlp"
momentum: 0.9
# solver mode: CPU or GPU
solver_mode: GPU